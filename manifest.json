{
  "manifest_version": 2,

  "name": "fake stackoverflow",
  "description": "Pretend you are visiting Stackoverflow.com ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://*.zhihu.com/*"],
    "css": ["stackoverflow.css"],
    "js": ["jquery.js", "main.js"]
  }],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}