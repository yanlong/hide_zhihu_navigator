{
  "manifest_version": 2,

  "name": "Hide Zhihu Navigator",
  "description": "Hide Zhihu Navigator",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://*.zhihu.com/*"],
    "css": ["stackoverflow.css"],
    "js": ["jquery.js", "main.js"]
  }]
}